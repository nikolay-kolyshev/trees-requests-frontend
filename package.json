{
    "name": "web-application",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "start": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "circular-check": "dpdm src/* -T true --tree false --warning false --tsconfig tsconfig.json --exit-code circular:1",
        "lint": "pnpm run eslint && pnpm run prettier:check",
        "lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix",
        "eslint": "eslint \"src/*.{ts,tsx}\"",
        "eslint:fix": "pnpm run eslint --fix",
        "prettier:check": "prettier --check .",
        "prettier:fix": "prettier --write .",
        "unsetup": "pnpm dlx rimraf node_modules node_modules/",
        "presetup": "pnpm run unsetup",
        "setup": "pnpm install --frozen-lockfile",
        "prereinstall": "pnpm run unsetup && pnpm dlx rimraf pnpm-lock.yaml",
        "reinstall": "pnpm install"
    },
    "dependencies": {
        "@abraham/reflection": "0.12.0",
        "@fontsource/roboto": "4.5.8",
        "@mui/icons-material": "5.11.16",
        "@mui/lab": "5.0.0-alpha.127",
        "@mui/material": "5.12.1",
        "@mui/styled-engine-sc": "5.12.0",
        "@swc/core": "1.3.46",
        "@vitejs/plugin-react": "3.1.0",
        "axios": "1.3.4",
        "effector": "^22.8.1",
        "effector-react": "^22.5.1",
        "effector-storage": "5.0.1",
        "formik": "2.2.9",
        "localforage": "1.10.0",
        "match-sorter": "6.3.1",
        "mui-image": "^1.0.7",
        "patronum": "^1.17.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-is": "18.2.0",
        "react-responsive": "9.0.2",
        "react-router-dom": "6.10.0",
        "reflect-metadata": "0.1.13",
        "rxjs": "7.8.0",
        "sort-by": "1.2.0",
        "styled-components": "5.3.9",
        "typedi": "0.10.0",
        "unplugin-swc": "1.3.2",
        "vite-plugin-mkcert": "1.14.0",
        "yup": "1.0.2"
    },
    "devDependencies": {
        "@effector/swc-plugin": "0.0.9",
        "@swc/helpers": "0.5.0",
        "@swc/plugin-styled-components": "1.5.56",
        "@types/mui-image": "1.0.1",
        "@types/node": "18.15.11",
        "@types/react": "18.0.28",
        "@types/react-dom": "18.0.11",
        "@types/styled-components": "5.1.26",
        "@typescript-eslint/eslint-plugin": "5.58.0",
        "@typescript-eslint/parser": "5.58.0",
        "dpdm": "3.13.0",
        "eslint": "8.38.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-effector": "0.10.5",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "prettier": "2.8.7",
        "typescript": "5.0.3",
        "vite": "4.2.1"
    },
    "description": "Веб-интерфейс для отправки заявок на лечение деревьев",
    "homepage": "https://github.com/nikolay-kolyshev/trees-requests-frontend#readme",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nikolay-kolyshev/trees-requests-frontend.git"
    },
    "bugs": {
        "url": "https://github.com/nikolay-kolyshev/trees-requests-frontend/issues"
    }
}
